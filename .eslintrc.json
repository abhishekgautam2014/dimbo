{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "plugins": [
      "prettier",
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "printWidth": 120
        }
      ]
    }
  }